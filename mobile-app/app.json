{
  "expo": {
    "name": "Super Menu",
    "slug": "super-menu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/launcher-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ndungutsecharles19.supermenu"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/launcher-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ndungutsecharles19.supermenu"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/launcher-icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
